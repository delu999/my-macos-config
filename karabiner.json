{
    "global": { "check_for_updates_on_startup": false },
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "manipulators": [
                            {
                                "description": "Change caps_lock to command+control+option+shift (meta key)",
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "left_shift",
                                        "modifiers": ["left_command", "left_control", "left_option"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                      "description": "Cmd+Opt+Ctrl+Shift+H to open spotlight clipboard history",
                      "manipulators": [
                        {
                          "from": {
                            "key_code": "h",
                            "modifiers": {
                              "mandatory": [
                                "command",
                                "option",
                                "control",
                                "shift"
                              ]
                            }
                          },
                          "to": [
                              {
                                  "hold_down_milliseconds": 200,
                                  "key_code": "spacebar",
                                  "modifiers": ["command"]
                              },
                              {
                                  "key_code": "4",
                                  "modifiers": ["command"]
                              }
                          ],
                          "type": "basic"
                        }
                      ]
                    },
                    {
                        "description": "meta + b Brave",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "b",
                                    "modifiers": { "mandatory": ["command", "option", "control", "shift"] }
                                },
                                "to": [{ "shell_command": "open -a 'Brave Browser'" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "meta + b Ghostty",
                        "manipulators": [
